		<dataSource>DataSource1</dataSource>
		<query>SELECT
	GeneralJournalBalance.ExtDimension1 AS Company,
	GeneralJournalBalance.ExtDimension1.Description AS CompanyID,
	GeneralJournalBalance.ExtDimension2 AS Document,
	GeneralJournalBalance.Currency,
	CASE
		WHEN DATEDIFF(GeneralJournalBalance.ExtDimension2.Date, &amp;EndOfPeriod, DAY) &lt;= 0
			THEN GeneralJournalBalance.AmountClosingBalance * -1
		ELSE 0
	END AS D0_FCY,
	CASE
		WHEN DATEDIFF(GeneralJournalBalance.ExtDimension2.Date, &amp;EndOfPeriod, DAY) &lt;= 0
			THEN GeneralJournalBalance.AmountRCClosingBalance * -1
		ELSE 0
	END AS D0,
	CASE
		WHEN DATEDIFF(GeneralJournalBalance.ExtDimension2.Date, &amp;EndOfPeriod, DAY) &lt;= &amp;Interval1
				AND DATEDIFF(GeneralJournalBalance.ExtDimension2.Date, &amp;EndOfPeriod, DAY) &gt;= 0
			THEN GeneralJournalBalance.AmountClosingBalance * -1
		ELSE 0
	END AS D1_FCY,
	CASE
		WHEN DATEDIFF(GeneralJournalBalance.ExtDimension2.Date, &amp;EndOfPeriod, DAY) &lt;= &amp;Interval1
				AND DATEDIFF(GeneralJournalBalance.ExtDimension2.Date, &amp;EndOfPeriod, DAY) &gt;= 0
			THEN GeneralJournalBalance.AmountRCClosingBalance * -1
		ELSE 0
	END AS D1,
	CASE
		WHEN DATEDIFF(GeneralJournalBalance.ExtDimension2.Date, &amp;EndOfPeriod, DAY) &lt;= &amp;Interval2
				AND DATEDIFF(GeneralJournalBalance.ExtDimension2.Date, &amp;EndOfPeriod, DAY) &gt; &amp;Interval1
			THEN GeneralJournalBalance.AmountClosingBalance * -1
		ELSE 0
	END AS D2_FCY,
	CASE
		WHEN DATEDIFF(GeneralJournalBalance.ExtDimension2.Date, &amp;EndOfPeriod, DAY) &lt;= &amp;Interval2
				AND DATEDIFF(GeneralJournalBalance.ExtDimension2.Date, &amp;EndOfPeriod, DAY) &gt; &amp;Interval1
			THEN GeneralJournalBalance.AmountRCClosingBalance * -1
		ELSE 0
	END AS D2,
	CASE
		WHEN DATEDIFF(GeneralJournalBalance.ExtDimension2.Date, &amp;EndOfPeriod, DAY) &lt;= &amp;Interval3
				AND DATEDIFF(GeneralJournalBalance.ExtDimension2.Date, &amp;EndOfPeriod, DAY) &gt; &amp;Interval2
			THEN GeneralJournalBalance.AmountClosingBalance * -1
		ELSE 0
	END AS D3_FCY,
	CASE
		WHEN DATEDIFF(GeneralJournalBalance.ExtDimension2.Date, &amp;EndOfPeriod, DAY) &lt;= &amp;Interval3
				AND DATEDIFF(GeneralJournalBalance.ExtDimension2.Date, &amp;EndOfPeriod, DAY) &gt; &amp;Interval2
			THEN GeneralJournalBalance.AmountRCClosingBalance * -1
		ELSE 0
	END AS D3,
	CASE
		WHEN DATEDIFF(GeneralJournalBalance.ExtDimension2.Date, &amp;EndOfPeriod, DAY) &gt; &amp;Interval3
			THEN GeneralJournalBalance.AmountClosingBalance * -1
		ELSE 0
	END AS D4_FCY,
	CASE
		WHEN DATEDIFF(GeneralJournalBalance.ExtDimension2.Date, &amp;EndOfPeriod, DAY) &gt; &amp;Interval3
			THEN GeneralJournalBalance.AmountRCClosingBalance * -1
		ELSE 0
	END AS D4,
	GeneralJournalBalance.AmountClosingBalance * -1 AS TotalD_FCY,
	GeneralJournalBalance.AmountRCClosingBalance * -1 AS TotalD,
	Companies.Description
FROM
	AccountingRegister.GeneralJournal.BalanceAndTurnovers(, , Auto, , , , ) AS GeneralJournalBalance
		LEFT JOIN Catalog.Companies AS Companies
		ON GeneralJournalBalance.ExtDimension1 = Companies.Ref
WHERE
	(GeneralJournalBalance.ExtDimension2 REFS Document.PurchaseInvoice
			OR GeneralJournalBalance.ExtDimension2 REFS Document.GeneralJournalEntry
				AND GeneralJournalBalance.ExtDimension2.ARorAP = VALUE(Enum.GJEntryType.AP)
			OR GeneralJournalBalance.ExtDimension2 REFS Document.PurchaseReturn)
{WHERE
	(GeneralJournalBalance.ExtDimension1 = &amp;Vendor)}</query>
