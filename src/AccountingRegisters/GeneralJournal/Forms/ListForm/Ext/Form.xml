			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	AccountingRegisterGeneralJournalRecordsWithExtDimensions.Period,
	AccountingRegisterGeneralJournalRecordsWithExtDimensions.Recorder,
    AccountingRegisterGeneralJournalRecordsWithExtDimensions.Recorder.Number,	
	AccountingRegisterGeneralJournalRecordsWithExtDimensions.Recorder.Company,
	VALUETYPE(AccountingRegisterGeneralJournalRecordsWithExtDimensions.Recorder) AS DocumentType,
	AccountingRegisterGeneralJournalRecordsWithExtDimensions.LineNumber,
	AccountingRegisterGeneralJournalRecordsWithExtDimensions.Account,
	AccountingRegisterGeneralJournalRecordsWithExtDimensions.Currency,
	CASE WHEN AccountingRegisterGeneralJournalRecordsWithExtDimensions.RecordType = VALUE(AccountingRecordType.Debit)
	Then AccountingRegisterGeneralJournalRecordsWithExtDimensions.Amount
	Else 0
	END AS AmountDr,
	CASE WHEN AccountingRegisterGeneralJournalRecordsWithExtDimensions.RecordType = VALUE(AccountingRecordType.Debit)
	Then AccountingRegisterGeneralJournalRecordsWithExtDimensions.AmountRC
	Else 0
	END AS AmountRCDr,
    CASE WHEN AccountingRegisterGeneralJournalRecordsWithExtDimensions.RecordType = VALUE(AccountingRecordType.Credit)
	Then AccountingRegisterGeneralJournalRecordsWithExtDimensions.Amount
	Else 0
	END AS AmountCr,
	CASE WHEN AccountingRegisterGeneralJournalRecordsWithExtDimensions.RecordType = VALUE(AccountingRecordType.Credit)
	Then AccountingRegisterGeneralJournalRecordsWithExtDimensions.AmountRC
	Else 0
	END AS AmountRCCr,
	AccountingRegisterGeneralJournalRecordsWithExtDimensions.Amount,
	AccountingRegisterGeneralJournalRecordsWithExtDimensions.AmountRC,
	AccountingRegisterGeneralJournalRecordsWithExtDimensions.RecordType,
	AccountingRegisterGeneralJournalRecordsWithExtDimensions.ExtDimension1 AS Company,
	AccountingRegisterGeneralJournalRecordsWithExtDimensions.ExtDimension2 AS Document,
	ChartOfAccounts.Description,
	Companies.Code AS CompanyCode,
	AccountingRegisterGeneralJournalRecordsWithExtDimensions.Memo
FROM
	AccountingRegister.GeneralJournal.RecordsWithExtDimensions AS AccountingRegisterGeneralJournalRecordsWithExtDimensions
		LEFT JOIN ChartOfAccounts.ChartOfAccounts AS ChartOfAccounts
		ON AccountingRegisterGeneralJournalRecordsWithExtDimensions.Account = ChartOfAccounts.Ref
		LEFT JOIN Catalog.Companies AS Companies
		ON AccountingRegisterGeneralJournalRecordsWithExtDimensions.ExtDimension1 = Companies.Ref</QueryText>
				<MainTable>AccountingRegister.GeneralJournal.RecordsWithExtDimensions</MainTable>
